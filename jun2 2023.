double pi = 3.14;

DateTime danas = DateTime.Today;
inc(pi, danas, niz[0]);
Console.WriteLine(danas.DayOfWeek);
Console.WriteLine(niz[0]);
Console.WriteLine(pi);

 private static void inc(double pi, DateTime dn, kompl k1)
 {
     pi++;
     dn.AddDays(1);
     k1.R++;
     k1.G++;
 }

